<p id="notice"><%= notice %></p>

<h1>Boring Airlines</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @boring_airlines.each do |boring_airline| %>
      <tr>
        <td><%= boring_airline.Content %></td>
        <td><%= link_to 'Show', boring_airline %></td>
        <td><%= link_to 'Edit', edit_boring_airline_path(boring_airline) %></td>
        <td><%= link_to 'Destroy', boring_airline, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Boring Airline', new_boring_airline_path %>
